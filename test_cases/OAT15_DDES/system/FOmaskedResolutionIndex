maskedCelikNuFieldIndex
{
    type    multiply;
    libs    (fieldFunctionObjects);

    fields
    (
        DESField
        CelikNuField
    );
    result          maskedCelikNuField;

    executeControl  timeStep;
    executeInterval 1;
    writeControl    writeTime;
    writeInterval   1;
    timeStart       4e-3;
}

maskedCelikEtaFieldIndex
{
    $maskedCelikNuFieldIndex;
    fields
    (
        DESField
        CelikEtaField
    );
    result          maskedCelikEtaField;
}


